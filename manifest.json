{
  "manifest_version": 2,
  "name": "Varanida",
  "description": "Decentralized Ads Cleaner",
  "version": "0.1",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "background": {
    "scripts": ["worker.js"]
  },
  "web_accessible_resources": ["empty.html", "empty.png", "empty.js"],
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*/*"
  ]
}
